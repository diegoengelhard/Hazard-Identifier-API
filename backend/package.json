{
  "name": "node-express-ts-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node bin/www",
    "dev": "nodemon --watch 'bin/**/*.ts' --watch 'routes/**/*.ts' --exec 'ts-node' bin/www",
