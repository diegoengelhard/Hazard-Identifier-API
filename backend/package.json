    "gen:data": "node src/utils/generate-data.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.0",
    "mongoose": "^8.7.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.6.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
