  "bigrams": [
    { "phrase": "highly flammable" },
    { "phrase": "extremely flammable" },
    { "phrase": "flammable liquid" },
    { "phrase": "flammable solid" },
    { "phrase": "oxidizing agent" },
    { "phrase": "corrosive to metals" },
    { "phrase": "causes skin burns" },
    { "phrase": "toxic if inhaled" },
    { "phrase": "toxic if swallowed" },
    { "phrase": "toxic to aquatic life" },
    { "phrase": "may cause cancer" },
    { "phrase": "may damage fertility" },
    { "phrase": "serious eye damage" },
    { "phrase": "chlorine tablets" },
    { "phrase": "pool shock" }
  ],

  "regex": [
    { "name": "percent solutions (simple)", "pattern": "\\b\\d{1,2}%\\s+(acid|peroxide|bleach)\\b" }
  ],

  "negations": [
    { "term": "non-toxic", "variants": ["nontoxic", "non toxic"], "impacts": ["toxic", "harmful"] },
    { "term": "water-based", "variants": ["water based", "waterborne"], "impacts": ["paint", "solvent"] },
