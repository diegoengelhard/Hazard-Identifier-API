    { "term": "latex paint", "impacts": ["paint"] },
    { "term": "empty container", "impacts": ["aerosol", "solvent", "paint", "bleach", "acid"] },
    { "term": "battery removed", "impacts": ["battery", "electronics"] }
  ],

