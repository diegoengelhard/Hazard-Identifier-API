    { "term": "oxidizer", "type": "technical", "variants": ["oxidising", "oxidizing agent"] },
    { "term": "corrosive", "type": "technical", "variants": ["corrodes metal", "corrodes metals", "skin burns"] },
    { "term": "toxic", "type": "technical", "variants": ["very toxic", "poisonous"] },
    { "term": "harmful", "type": "technical", "variants": ["noxious"] },
    { "term": "irritant", "type": "technical", "variants": ["eye irritation", "skin irritation", "irritating"] },
